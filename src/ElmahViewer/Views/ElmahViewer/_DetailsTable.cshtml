@model dynamic
 @if (Model.Collection.Count > 0)
 {
     <div id="@Model.Name.Replace(" ", string.Empty)">
         <table class="table table-condensed table-striped table-bordered">
             <tr ><th colspan="2">@Model.Name</th></tr>
             <tr>
                 <th>Name</th>
                 <th>Value</th>
             </tr>
    
             @for (int i = 0; i < Model.Collection.Count; i++)
             {
                 <tr>
                     <td>@Model.Collection.GetKey(i)</td>
                     <td>@Model.Collection.Get(i)</td>
                 </tr>
             }
         </table>
     </div>
 }
